meme central:memescript/0.0.1/compiler
requires test

// -- module functions --


main: fun() {
  test.assertEqual([3,4,5].range(0, 2), [3,4], "range 1");
  test.assertEqual([3,4,5,6].range(1, -1), [4,5], "range 2");

  var x = [{:id: 2}, {:id: 10}, {:id: 5}];
  test.assertEqual(x.ordered_by(fun(x,y) { x[:id] < y[:id] }), [{:id: 2}, {:id: 5}, {:id: 10}], "order");
}
