 -parent: ObjectBehavior        -parent: Object                       -delegate: None
 -dict: {compiled_module}       -size: <all basic objects>            -Object: ..
       //instantiate}           -dict: {Object,Array,...}             -Array:  ..
                                -compiled_module:xx                   ...

                                 FooModule                             foo_module_instance
                                 *VT: ModuleBehavior/deleg:None        *VT: FooModule
                                 -parent: KernelModule                 -delegate: module_kernel_instance
